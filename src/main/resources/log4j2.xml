<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
    <Appenders>
        <Console target="SYSTEM_OUT" name="Console">
    <PatternLayout pattern="%d{HH:mm:ss.SSS}[%t]%-5level%logger{36}-%msg%n"/>
</Console>
</Appenders>
    <Loggers>
        <!--Root Logger-->
        <Root level="all">
        <appender-ref ref="Console"/>
        </Root>
    </Loggers>




</Configuration>